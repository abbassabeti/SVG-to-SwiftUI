{
  "name": "svg-to-swiftui-core",
  "version": "0.3.5",
  "description": "SVG to SwiftUI Shape converter core package. Will transform the raw SVG code into SwiftUI Shape via the JavaScript API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "svg",
    "swiftui",
    "transpiler",
    "swift",
    "converter"
  ],
  "author": {
    "name": "Antoni Silvestrovic",
    "email": "antoni.silvestrovic@gmail.com",
    "url": "https://antoni.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quassum/svg-to-swiftui-core.git"
  },
  "bugs": {
    "url": "https://github.com/quassum/svg-to-swiftui-core/issues"
  },
  "homepage": "https://github.com/quassum/svg-to-swiftui-core#readme",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify --external react",
    "clean": "rimraf dist",
    "test": "jest",
    "coverage": "jest --ci --coverage",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix",
    "format": "prettier --write ./src --loglevel error"
  },
  "devDependencies": {
    "@quassum/prettier-config": "^1.3.0",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "@types/hast": "^2.3.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.3",
    "@types/prettier": "^2.7.3",
    "@types/svg-parser": "^2.0.6",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.55.0",
    "gts": "^3.1.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "svg-parser": "^2.0.4",
    "svg-pathdata": "^6.0.3"
  }
}
