{
  "name": "figma-to-swiftui",
  "description": "Convert Figma Paths to SwiftUI",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format iife --target es5",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix",
    "format": "prettier --write ./src --loglevel error"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.82.0",
    "@quassum/prettier-config": "^1.3.0",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "@types/hast": "^2.3.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.3",
    "@types/prettier": "^2.7.3",
    "@types/svg-parser": "^2.0.6",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "svg-to-swiftui-core": "workspace:*"
  },
  "license": "MIT",
  "author": {
    "name": "Antoni Silvestrovic",
    "email": "antoni.silvestrovic@gmail.com",
    "url": "https://antoni.ai"
  }
}
